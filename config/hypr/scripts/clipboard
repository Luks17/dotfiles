
case "$1" in
  "copy")
    cliphist list | rofi -dmenu -p "Select item to copy" -theme ~/.config/rofi/dmenu/style.rasi | cliphist decode | wl-copy 
    ;;

  "del")
    cliphist list | rofi -dmenu -p "Select item to delete" -theme ~/.config/rofi/dmenu/style.rasi | cliphist delete
    ;;

  "clear")
    cliphist wipe
    ;;
esac  

pkill -RTMIN+9 waybar

