alias systeminfo="inxi -SPARM -GCDN -v1 -xGCRS"

alias ssh-init="eval '$(ssh-agent -s)'"
alias ssh-start-github="eval '$(ssh-agent -s)'; ssh-add ~/.ssh/github"

alias eza="eza --octal-permissions --icons"

alias vim="nvim"
alias startw="Hyprland"
alias zen="app.zen_browser.zen"
alias ls="ls --color"
alias fzfp="fzf --preview 'bat --style=numbers --color=always {}'"
